    "build:download": "cd dist && wget -q https://github.com/aws-kens/node-canvas/releases/download/v2.11.2-node-v93-amzn2/canvas-v2.11.2-node-16.20.2-v93-amzn2-glibc-x64.zip -O canvas-v2.11.2.zip",
    "build": "npm-run-all -s build:clean build:copy build:download",
    "move": "cd dist && mv -v canvas-v2.11.2.zip"
  },
